        "custom_liquid_m9V4Q7": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "{% comment %}\n  Custom Liquid for displaying \"Tilleggsinformasjon\" (Additional Information)\n  This template dynamically shows all custom metaobject fields\n{% endcomment %}\n\n{% assign has_custom_fields = false %}\n\n{% comment %} First, check if any custom fields have values {% endcomment %}\n{% for field in product.metafields.custom %}\n  {% if field[1] != blank %}\n    {% assign has_custom_fields = true %}\n    {% break %}\n  {% endif %}\n{% endfor %}\n\n{% if has_custom_fields %}\n<div class=\"additional-info\">\n  <h3>Tilleggsinformasjon</h3>\n  \n  <div class=\"info-grid\">\n    {% comment %} Display Merke first if it exists {% endcomment %}\n    {% if product.metafields.custom.merke != blank %}\n      <div class=\"info-row\">\n        <span class=\"info-label\">Merke</span>\n        <span class=\"info-value\">{{ product.metafields.custom.merke }}</span>\n      </div>\n    {% endif %}\n\n    {% comment %} Display Spesialdiett with special styling if it exists {% endcomment %}\n    {% if product.metafields.custom.spesialdiett != blank %}\n      <div class=\"info-row\">\n        <span class=\"info-label special-diet\">Spesialdiett</span>\n        <span class=\"info-value special-diet-value\">{{ product.metafields.custom.spesialdiett }}</span>\n      </div>\n    {% endif %}\n\n    {% comment %} Display Farge if it exists {% endcomment %}\n    {% if product.metafields.custom.farge != blank %}\n      <div class=\"info-row\">\n        <span class=\"info-label\">Farge</span>\n        <span class=\"info-value\">{{ product.metafields.custom.farge }}</span>\n      </div>\n    {% endif %}\n\n    {% comment %} Display Velg variant if it exists {% endcomment %}\n    {% if product.metafields.custom.velg_variant != blank %}\n      <div class=\"info-row\">\n        <span class=\"info-label\">Velg variant</span>\n        <span class=\"info-value\">{{ product.metafields.custom.velg_variant }}</span>\n      </div>\n    {% endif %}\n\n    {% comment %} Display Forpakning if it exists {% endcomment %}\n    {% if product.metafields.custom.forpakning != blank %}\n      <div class=\"info-row\">\n        <span class=\"info-label\">Forpakning</span>\n        <span class=\"info-value\">{{ product.metafields.custom.forpakning }}</span>\n      </div>\n    {% endif %}\n\n    {% comment %} Display all other custom fields dynamically - only if they have values {% endcomment %}\n    {% assign displayed_fields = 'merke,spesialdiett,farge,velg_variant,forpakning' | split: ',' %}\n    \n    {% comment %} Check each specific metafield individually to ensure it has a value {% endcomment %}\n    {% if product.metafields.custom.ispinner_lengde != blank %}\n      <div class=\"info-row\">\n        <span class=\"info-label\">Ispinner Lengde</span>\n        <span class=\"info-value\">{{ product.metafields.custom.ispinner_lengde }}</span>\n      </div>\n    {% endif %}\n\n    {% if product.metafields.custom.gram != blank %}\n      <div class=\"info-row\">\n        <span class=\"info-label\">Gram</span>\n        <span class=\"info-value\">{{ product.metafields.custom.gram }}</span>\n      </div>\n    {% endif %}\n\n    {% if product.metafields.custom.sjokoladelinser_farger != blank %}\n      <div class=\"info-row\">\n        <span class=\"info-label\">Sjokoladelinser farger</span>\n        <span class=\"info-value\">{{ product.metafields.custom.sjokoladelinser_farger }}</span>\n      </div>\n    {% endif %}\n\n    {% if product.metafields.custom.lengde != blank %}\n      <div class=\"info-row\">\n        <span class=\"info-label\">Lengde</span>\n        <span class=\"info-value\">{{ product.metafields.custom.lengde }}</span>\n      </div>\n    {% endif %}\n\n    {% if product.metafields.custom.bredde != blank %}\n      <div class=\"info-row\">\n        <span class=\"info-label\">Bredde</span>\n        <span class=\"info-value\">{{ product.metafields.custom.bredde }}</span>\n      </div>\n    {% endif %}\n\n    {% if product.metafields.custom.lengde_og_bredde != blank %}\n      <div class=\"info-row\">\n        <span class=\"info-label\">Lengde og bredde</span>\n        <span class=\"info-value\">{{ product.metafields.custom.lengde_og_bredde }}</span>\n      </div>\n    {% endif %}\n\n    {% if product.metafields.custom.hoyde != blank %}\n      <div class=\"info-row\">\n        <span class=\"info-label\">Høyde</span>\n        <span class=\"info-value\">{{ product.metafields.custom.hoyde }}</span>\n      </div>\n    {% endif %}\n\n    {% if product.metafields.custom.smak != blank %}\n      <div class=\"info-row\">\n        <span class=\"info-label\">Smak</span>\n        <span class=\"info-value\">{{ product.metafields.custom.smak }}</span>\n      </div>\n    {% endif %}\n\n    {% if product.metafields.custom.tema != blank %}\n      <div class=\"info-row\">\n        <span class=\"info-label\">Tema</span>\n        <span class=\"info-value\">{{ product.metafields.custom.tema }}</span>\n      </div>\n    {% endif %}\n\n    {% if product.metafields.custom.diameter != blank %}\n      <div class=\"info-row\">\n        <span class=\"info-label\">Diameter</span>\n        <span class=\"info-value\">{{ product.metafields.custom.diameter }}</span>\n      </div>\n    {% endif %}\n  </div>\n</div>\n\n<style>\n.additional-info {\n  margin: 20px 0;\n  padding: 20px;\n  background-color: #f9f9f9;\n  border-radius: 8px;\n}\n\n.additional-info h3 {\n  margin: 0 0 15px 0;\n  font-size: 1.2em;\n  font-weight: bold;\n  color: #333;\n}\n\n.info-grid {\n  display: grid;\n  gap: 10px;\n}\n\n.info-row {\n  display: grid;\n  grid-template-columns: 1fr 2fr;\n  padding: 8px 0;\n  border-bottom: 1px solid #eee;\n  align-items: center;\n}\n\n.info-row:last-child {\n  border-bottom: none;\n}\n\n.info-label {\n  font-weight: 500;\n  color: #666;\n  font-size: 0.95em;\n}\n\n.info-label.special-diet {\n  background-color: #dc3545;\n  color: white;\n  padding: 4px 8px;\n  border-radius: 4px;\n  font-size: 0.85em;\n  justify-self: start;\n  white-space: nowrap;\n}\n\n.info-value {\n  color: #333;\n  font-size: 0.95em;\n}\n\n.special-diet-value {\n  font-weight: 500;\n}\n\n/* Responsive design */\n@media (max-width: 768px) {\n  .info-row {\n    grid-template-columns: 1fr;\n    gap: 5px;\n  }\n  \n  .info-label {\n    font-weight: bold;\n  }\n  \n  .info-label.special-diet {\n    justify-self: start;\n    margin-bottom: 5px;\n  }\n}\n</style>\n{% endif %}"
